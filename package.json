{
  "name": "heritage-navigator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:staging": "NEXT_PUBLIC_ENVIRONMENT=staging next build",
    "start": "next start",
    "lint": "next lint",
    "import-mock-data": "ts-node scripts/import-mock-data.ts",
    "update-types": "npx supabase gen types typescript --linked > types/supabase.ts",
    "reset-db": "./scripts/reset-local-db.sh",
    "dev-workflow": "./scripts/dev-workflow.sh",
    "rollback": "./scripts/rollback.sh",
    "type-check": "tsc --noEmit",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:status": "supabase status",
    "test": "jest",
    "test:db": "jest --testPathPattern=tests/db",
    "prebuild": "npm run type-check",
    "test:ci": "jest --ci --passWithNoTests",  
    "db:backup": "mkdir -p backups && supabase db dump -f backups/backup_$(date +%Y%m%d_%H%M%S).sql",
    "db:push": "npm run db:backup && supabase db push",
    "validate": "npm run type-check && npm run test:ci"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.1.0",
    "@shadcn/ui": "^0.0.4",
    "@supabase/supabase-js": "^2.46.1",
    "@types/leaflet": "^1.9.14",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "framer-motion": "^11.11.17",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.460.0",
    "next": "^14.2.18",
    "next-intl": "3.5.4",
    "next-pwa": "5.6.0",
    "next-themes": "^0.4.3",
    "postcss": "^8",
    "react": "^18",
    "react-dom": "^18",
    "react-leaflet": "^4.2.1",
    "tailwind-merge": "^2.5.4",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  },
  "devDependencies": {
    "@netlify/plugin-nextjs": "^5.8.1",
    "@types/jest": "^29.5.0",
    "dotenv": "^16.4.5",
    "eslint": "^8",
    "eslint-config-next": "14.0.3",
    "jest": "^29.5.0",
    "supabase": "^1.223.10",
    "ts-node": "^10.9.2"
  }
}